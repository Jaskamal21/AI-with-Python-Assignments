   "source": [
    "def adjacency_list(graph):\n",
    "    for node, neighbors in graph.items():\n",
    "        neighbors_list = \", \".join(neighbors)\n",
    "        print(f\"{node}: {neighbors_list}\")\n",
    "\n",
    "graph = {\n",
    "    \"A\": [\"B\", \"C\"],\n",
    "    \"B\": [\"A\", \"D\"],\n",
    "    \"C\": [\"A\"],\n",
    "    \"D\": [\"B\"]\n",
    "}\n",
    "\n",
    "adjacency_list(graph)\n"
   ]
  },
